    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "ms-rest-azure": "^2.6.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-azure-ad-oauth2": "0.0.4",
